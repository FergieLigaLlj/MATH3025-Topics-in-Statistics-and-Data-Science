    "from random import seed\n",
    "seed(39)\n",
    "samples = np.random.uniform(0,1,10000)\n",
    "pro = ((2+samples)**125)*((1-samples)**38)*(samples**34)\n",
    "pro = pro/(sum(pro))\n",
    "resampling = np.random.choice(samples,size=10000,p=pro)\n",
    "print(resampling)"
   ]
